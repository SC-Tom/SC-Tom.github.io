{
  "extends": "airbnb/base",
  "rules": {
    "comma-dangle": [
      1,
      "never"
    ],
    "no-unused-vars": 1,
    "object-shorthand": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "no-restricted-syntax": [
      0,
      "ForInStatement"
    ],
    "no-console": 0,
    "import/no-extraneous-dependencies": 0
  },
  "globals": {
    "APP_CONFIG": true,
    "describe": true,
    "it": true,
    "beforeEach": true,
    "afterEach": true,
    "browser": true,
    "sinon": true,
    "expect": true,
    "before": true,
    "after": true
  }
}
